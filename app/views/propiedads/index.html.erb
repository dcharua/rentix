  <div class="wrapper wrapper-content  animated fadeInRight">
      <div class="row">
          <div class="col-sm-12">
              <div class="ibox">
                  <div class="ibox-content">
                      <span class="text-muted small pull-right">Ultima modificacion <i class="fa fa-clock-o"></i> <%=@actualizado%></span>
                      <h2>Propiedades</h2>
                      <p>
                          Estos son todas tus propiedades, haz click en editar para modificar sus datos
                      </p>
                      <div class="input-group">
                          <input type="text" placeholder="Search client " class="input form-control">
                          <span class="input-group-btn">
                              <button type="button" class="btn btn btn-primary"> <i class="fa fa-search"></i> Search</button>
                          </span>
                      </div>
                      <div class="clients-list">
                          <ul class="nav nav-tabs">
                              <span class="pull-right small text-muted">Tienes <%=@propiedads.count%> Propiedades</span>
                              <li class="active"><a data-toggle="tab" href="#tab-1"><i class="fa fa-home"></i> Propiedades</a></li>
                          </ul>
                          <div class="tab-content">
                              <div id="tab-1" class="tab-pane active">
                                  <div class="full-height-scroll">
                                      <div class="table-responsive">
                                          <table class="table table-striped table-hover">
                                              <tbody>
                                                <tr>
                                                    <td><strong>Título</strong></td>
                                                    <td><strong>Calle</strong></td>
                                                    <td><strong>Estado</strong></td>
                                                    <td><strong>CP</strong></td>
                                                    <td><strong>Municipio</strong></td>
                                                    <td><strong>Acciones</strong></td>
                                                </tr>
                                                <% @propiedads.each do |propiedad| %>
                                              <tr>
                                                  <td><%= propiedad.nombre%></td>
                                                  <td> <%= propiedad.calle%></td>
                                                  <td><%= propiedad.estado%></td>
                                                  <td> <%= propiedad.cp%></td>
                                                  <td><%= propiedad.municipio%></td>
                                                  <td class="client-status">  <%= link_to "Editar", edit_propiedad_path(propiedad), class: "btn btn-xs btn-primary" %>
                                                    <%= link_to "Borrar", propiedad_path(propiedad), method: :delete,
                                                                                      data: { confirm: "¿Estas seguro que quieres borrar a este propiedad?"},
                                                                                      class: "btn btn-xs btn-danger" %></td>
                                              </tr>
                                              <%end%>
                                              </tbody>
                                          </table>
                                      </div>
                                  </div>
                              </div>
                          </div>

                      </div>
                  </div>
              </div>
          </div>
        </div>
    </div>
