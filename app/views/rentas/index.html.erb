  <div class="wrapper wrapper-content  animated fadeInRight">
      <div class="row">
          <div class="col-sm-12">
              <div class="ibox">
                  <div class="ibox-content">
                      <span class="text-muted small pull-right">Ultima modificacion <i class="fa fa-clock-o"></i> </span>
                      <h2>Rentas</h2>
                      <p>
                          Estos son todas tus rentas, haz click en editar para modificar sus datos
                      </p>
                      <div class="input-group">
                          <input type="text" placeholder="Search client " class="input form-control">
                          <span class="input-group-btn">
                              <button type="button" class="btn btn btn-primary"> <i class="fa fa-search"></i> Search</button>
                          </span>
                      </div>
                      <div class="clients-list">
                          <ul class="nav nav-tabs">
                              <span class="pull-right small text-muted">Tienes  Rentas</span>
                              <li class="active"><a data-toggle="tab" href="#tab-1"><i class="fa fa-home"></i> Rentas</a></li>
                          </ul>
                          <div class="tab-content">
                              <div id="tab-1" class="tab-pane active">
                                  <div class="full-height-scroll">
                                      <div class="table-responsive">
                                          <table class="table table-striped table-hover">
                                              <tbody>
                                                <tr>
                                                    <td><strong>Inquilino</strong></td>
                                                    <td><strong>Propiedad</strong></td>
                                                    <td><strong>Mensualidad</strong></td>
                                                    <td><strong>Inicio</strong></td>
                                                    <td><strong>fin</strong></td>
                                                    <td><strong>Acciones</strong></td>
                                                </tr>
                                                <%@rentas.each do |renta|%>
                                              <tr>
                                                  <td><%= get_inquilinos(renta.inquilino_id).nombre%></td>
                                                  <td> <%= get_propiedads(renta.propiedad_id).nombre%></td>
                                                  <td><%= renta.costo%></td>
                                                  <td> <%= renta.inicio%></td>
                                                  <td><%= renta.final%></td>
                                                  <td class="client-status">  <%= link_to "Editar", edit_renta_path(renta), class: "btn btn-xs btn-primary" %>
                                                    <%= link_to "Borrar", renta_path(renta), method: :delete,
                                                                                      data: { confirm: "Â¿Estas seguro que quieres borrar esta renta?"},
                                                                                      class: "btn btn-xs btn-danger" %></td>
                                              </tr>
                                              <%end%>
                                              </tbody>
                                          </table>
                                      </div>
                                  </div>
                              </div>
                          </div>

                      </div>
                  </div>
              </div>
          </div>
        </div>
    </div>
