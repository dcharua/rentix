                            <td> N/A </td>
                              <% else%>
                            <td> <%=gastoM.propiedad.to_label%></td>
                              <% end%>
                            <td><%= gastoM.monto%></td>
                            <td> <%= gastoM.fecha%></td>
                            <td> <%= gastoM.comentarios%></td>
