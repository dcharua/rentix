                        <tbody>
                          <%  if @gastosM  %>
                        <%  if @gastosM.size > 0  %>
                          <tr>
                              <td><strong>Propiedad</strong></td>
                              <td><strong>Monto</strong></td>
                              <td><strong>Fecha</strong></td>
                              <td><strong>Comentarios</strong></td>
                              <td><strong>Acciones</strong></td>
                          </tr>
                          <% @gastosM.each do |gastoM|  %>
                        <tr>
                          <% if gastoM.propiedads_id.nil?%>
                            <td> N/A </td>
                              <% else%>
                            <td> <%= get_propiedads(gastoM.propiedads_id).to_label%></td>
                              <% end%>
                            <td><%= gastoM.monto%></td>
                            <td> <%= gastoM.fecha%></td>
                            <td> <%= gastoM.comentarios%></td>
                            <td class="client-status">  <%= link_to "Editar", edit_gasto_path(gastoM), class: "btn btn-xs btn-primary" %>
                              <%= link_to "Borrar", gasto_path(gastoM), method: :delete,
                                                                data: { confirm: "¿Estas seguro que quieres borrar a este gasto?"},
                                                                class: "btn btn-xs btn-danger" %></td>
                        </tr>
                        <%end%>
